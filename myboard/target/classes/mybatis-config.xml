<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 마이바티스 작동 규칙 정의 -->
	<settings>
		<setting name="cacheEnabled" value="false"/>				<!-- Mybatis cache 사용 여부 -->
		<setting name="useGeneratedKeys" value="false"/>			<!-- 생성 키에 대한 JDBC 지원 허용 여부 -->
	</settings>
	
	<typeAliases>
		<typeAlias type="com.test.board.domain.BoardVO" alias="BoardVO" />
		<typeAlias type="com.test.board.domain.ReplyVO" alias="ReplyVO" />
		<typeAlias type="com.test.board.domain.MessageVO" alias="MessageVO" />
	</typeAliases>
	
</configuration>